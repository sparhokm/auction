{
    "name": "sparhokm/demo-auction",
    "type": "project",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/annotations": "^1.13",
        "doctrine/migrations": "^3.4",
        "doctrine/orm": "^2.9",
        "finesse/swiftmailer-defaults-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "laminas/laminas-config-aggregator": "^1.6",
        "middlewares/negotiation": "^2.0",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.2",
        "sentry/sdk": "^3.1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/cache": "^5.3",
        "symfony/config": "^5.3",
        "symfony/console": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/validator": "^5.3",
        "twig/twig": "^3.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "doctrine/data-fixtures": "^1.5",
        "overtrue/phplint": "^3.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
          "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint" : "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
        "app": "php bin/app.php --ansi"
    }
}
